@{
    Layout = null;
}

<!DOCTYPE html>

<html ng-app="businessIncentivesApp">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Business Incentives</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/utils")
    <link rel="icon" type="image/png" href="/favicon.png" />
    <!--[if IE]>
    <link rel="shortcut icon" href="/favicon.ico" type="image/vnd.microsoft.icon" />
    <![endif]-->
    <script type="text/javascript">
        Utils.constants.currentYear = '@DateTime.Now.Year';
    </script>

</head>
<body ng-controller="rootController">
    <bi-top-bar></bi-top-bar>

    <div class="container body-content">
        <div class="page {{ pageClass }}" ng-view></div>

        <bi-footer></bi-footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/app")

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCnY5RLlSRZr1k4uHejhMfKRWBIpbdrz5U&libraries=places"></script>

</body>
</html>